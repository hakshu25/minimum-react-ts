{
  "name": "minimum-react-ts",
  "version": "1.0.0",
  "scripts": {
    "prebuild": "tsc --noEmit",
    "build": "vite build",
    "start": "vite --port 8080",
    "test": "jest",
    "test:ci": "jest --coverage --updateSnapshot --ci",
    "test:e2e": "cypress open",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prettier": "prettier --write '**/*.{js,ts,tsx,md,json,html,scss}'",
    "prettier:check": "yarn prettier --check",
    "codecheck": "yarn lint && yarn prettier:check",
    "codefix": "yarn lint:fix && yarn prettier",
    "postinstall": "make postinstall"
  },
  "main": "index.js",
  "repository": "https://github.com/flyhighair/minimum-react-ts.git",
  "author": "hakshu <flyhighup.air25@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 16"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@testing-library/cypress": "9.0.0",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.2.6",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "@vitejs/plugin-react": "^3.0.0",
    "babel-jest": "29.3.1",
    "css-loader": "6.7.3",
    "cypress": "12.3.0",
    "esbuild-loader": "2.21.0",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-react": "7.32.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "lint-staged": "13.1.0",
    "prettier": "2.8.3",
    "sass": "1.57.1",
    "sass-loader": "13.2.0",
    "style-loader": "3.3.1",
    "ts-jest": "29.0.5",
    "typescript": "4.9.4",
    "vite": "^4.0.0"
  },
  "resolutions": {
    "node-forge": "1.3.1"
  }
}
