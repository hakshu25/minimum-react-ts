{
  "name": "minimum-react-ts",
  "version": "1.0.0",
  "scripts": {
    "prebuild": "tsc --noEmit",
    "build": "webpack",
    "start": "webpack serve",
    "test": "jest",
    "test:ci": "jest --coverage --updateSnapshot --ci",
    "test:e2e": "cypress open",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prettier": "prettier --write '**/*.{js,ts,tsx,md,json,html,scss}'",
    "prettier:check": "yarn prettier --check",
    "codecheck": "yarn lint && yarn prettier:check",
    "codefix": "yarn lint:fix && yarn prettier",
    "postinstall": "make postinstall"
  },
  "main": "index.js",
  "repository": "https://github.com/flyhighair/minimum-react-ts.git",
  "author": "hakshu <flyhighup.air25@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 14"
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.15.6",
    "@babel/preset-react": "7.14.5",
    "@testing-library/cypress": "8.0.1",
    "@testing-library/react": "12.1.0",
    "@types/jest": "27.0.1",
    "@types/react": "17.0.21",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "4.31.0",
    "@typescript-eslint/parser": "4.31.0",
    "babel-jest": "27.1.1",
    "css-loader": "6.2.0",
    "cypress": "8.3.1",
    "esbuild-loader": "2.15.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "7.25.2",
    "html-webpack-plugin": "5.3.2",
    "husky": "7.0.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.1.1",
    "lint-staged": "11.1.2",
    "prettier": "2.4.0",
    "sass": "1.39.2",
    "sass-loader": "12.1.0",
    "style-loader": "3.2.1",
    "ts-jest": "27.0.5",
    "typescript": "4.4.3",
    "webpack": "5.52.1",
    "webpack-cli": "4.8.0",
    "webpack-dev-server": "4.2.0"
  },
  "resolutions": {
    "node-forge": "0.10.0"
  }
}
