{
  "name": "minimum-react-ts",
  "version": "1.0.0",
  "scripts": {
    "prebuild": "tsc --noEmit",
    "build": "vite build",
    "start": "vite --port 8080",
    "test": "jest",
    "test:ci": "jest --coverage --updateSnapshot --ci",
    "test:e2e": "cypress open",
    "lint": "eslint 'src/**/*.{ts,tsx}' 'e2e/**/*.{js,ts}'",
    "lint:fix": "yarn lint --fix",
    "prettier": "prettier --write '**/*.{js,ts,tsx,md,json,html,scss}'",
    "prettier:check": "prettier --check '**/*.{js,ts,tsx,md,json,html,scss}'",
    "codecheck": "yarn lint && yarn prettier:check",
    "codefix": "yarn lint:fix && yarn prettier",
    "postinstall": "make postinstall"
  },
  "main": "index.js",
  "repository": "https://github.com/hakshu25/minimum-react-ts.git",
  "author": "hakshu25 <hakshu25@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 16"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.22.10",
    "@babel/preset-react": "7.22.5",
    "@testing-library/cypress": "9.0.0",
    "@testing-library/react": "14.0.0",
    "@types/jest": "29.5.4",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "@vitejs/plugin-react": "^4.0.0",
    "babel-jest": "29.6.2",
    "cypress": "12.17.4",
    "eslint": "8.47.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-react": "7.33.2",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.6.2",
    "jest-environment-jsdom": "29.6.2",
    "lint-staged": "14.0.0",
    "prettier": "3.0.2",
    "sass": "1.66.1",
    "ts-jest": "29.1.1",
    "typescript": "5.1.6",
    "vite": "^4.0.0"
  }
}
