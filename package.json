{
  "name": "minimum-react-ts",
  "version": "1.0.0",
  "scripts": {
    "prebuild": "tsc --noEmit",
    "build": "webpack",
    "start": "webpack serve",
    "test": "jest",
    "test:ci": "jest --coverage --updateSnapshot --ci",
    "test:e2e": "cypress open",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prettier": "prettier --write '**/*.{js,ts,tsx,md,json,html,scss}'",
    "prettier:check": "yarn prettier --check",
    "codecheck": "yarn lint && yarn prettier:check",
    "codefix": "yarn lint:fix && yarn prettier",
    "postinstall": "make postinstall"
  },
  "main": "index.js",
  "repository": "https://github.com/flyhighair/minimum-react-ts.git",
  "author": "hakshu <flyhighup.air25@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 16"
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@testing-library/cypress": "8.0.2",
    "@testing-library/react": "12.1.3",
    "@types/jest": "27.4.0",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "babel-jest": "27.5.1",
    "css-loader": "6.6.0",
    "cypress": "9.4.1",
    "esbuild-loader": "2.18.0",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "7.28.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "lint-staged": "12.3.3",
    "prettier": "2.5.1",
    "sass": "1.49.7",
    "sass-loader": "12.4.0",
    "style-loader": "3.3.1",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5",
    "webpack": "5.68.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.7.4"
  },
  "resolutions": {
    "node-forge": "1.0.0"
  }
}
